<wpml-config>
    <admin-texts>
        <key name="financepro">
            <key name="phone" />
            <key name="address" />
            <key name="hour" />
            <key name="copyright_text" />
            <key name="header_btn_txt" />
            <key name="error_title" />
            <key name="error_text1" />
            <key name="error_text2" />
            <key name="error_buttontext" />
        </key>
    </admin-texts>
