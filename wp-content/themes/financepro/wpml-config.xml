        <custom-type translate="1">fin_service</custom-type>
        <custom-type translate="1">fin_case</custom-type>
        <custom-type translate="1">fin_team</custom-type>
        <custom-type translate="1">fin_portfolio</custom-type>
        <custom-type translate="1">fin_testimonial</custom-type>
    </custom-types>
